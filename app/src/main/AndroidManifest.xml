<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ejercicio3" >

    <!--uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" /-->

    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/camara"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icono"
        android:supportsRtl="true"
        android:theme="@style/Theme.Ejercicio3" >


        <activity android:name=".ActivityCombo" />

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"/>

        <activity
            android:name=".MapsActivity"/>
            <!-->android:label="@string/title_activity_maps" /-->
        <!--activity android:name="com.example.ejercicio3.ActivityPhoto" /-->
        <activity android:name="com.example.ejercicio3.ActivityListView" />
        <activity android:name="com.example.ejercicio3.ActivityConsulta" />
        <activity android:name="com.example.ejercicio3.ActivityIngresar" />

        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>